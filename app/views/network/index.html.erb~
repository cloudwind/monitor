<script> 
$(document).ready(function(){
	Morris.Area({
  		element: 'network-chart',
  		data:[
			<% (@data.count-1).downto(1).each do |i| %>
			{y: "<%= @data[i]["created_at"]%>",a: <%= (@data[i]["BandwidthTran"].to_i - @data[i-1]["BandwidthTran"].to_i)/2**10%>,b: <%= (@data[i]["BandwidthRec"].to_i - @data[i-1]["BandwidthRec"].to_i)/2**10%> }			
			<% if i != 0 %>
			<%= ',' %>
			<% end %>
			<% end %>
  		],
  		xkey: 'y',
  		ykeys: ['a','b'],
  		labels: ['Bandwidth Transmit', 'Bandwidth Receive'],
		hideHover: 'auto',
	});
	Morris.Area({
  		element: 'networkp-chart',
  		data:[
			<% (@data.count-1).downto(1).each do |i| %>
			{y: "<%= @data[i]["created_at"]%>",a: <%= @data[i]["BandwidthTranP"].to_i - @data[i-1]["BandwidthTranP"].to_i%>,b: <%= @data[i]["BandwidthRecP"].to_i - @data[i-1]["BandwidthRecP"].to_i%> }			
			<% if i != 0 %>
			<%= ',' %>
			<% end %>
			<% end %>
  		],
  		xkey: 'y',
  		ykeys: ['a','b'],
  		labels: ['Bandwidth Transmit', 'Bandwidth Receive'],
		hideHover: 'auto',
	});
});
</script>
